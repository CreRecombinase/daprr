main: main.o controller.o logistic.o 
	$(CXX)  $(CXX11STD) -O3 main.o controller.o logistic.o  -lm -lgsl -lgslcblas -lboost_iostreams -lz  -o torus
static: main.o controller.o logistic.o
	$(CXX)  $(CXX11STD) -O3 main.o controller.o logistic.o  -lm -lgsl -lgslcblas -lboost_iostreams -lz -static -o torus.static
main.o: main.cc 
	$(CXX) $(CXX11STD) -c  main.cc
controller.o: controller.cc classdef.h
	$(CXX) $(CXX11STD)  -c  controller.cc
logistic.o: logistic.cc logistic.h
	$(CXX) $(CXX11STD) -c logistic.cc -fpermissive
clean:
	rm *.o torus	
